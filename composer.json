{
    "name": "valerialevenets94/proxmox-low-battery-shutdown",
    "description": "The purpose of this project is to shutdown Proxmox VE node if UPS battery level is low",
    "type": "project",
    "require": {
        "php": "^8.3",
        "corsinvest/cv4pve-api-php": "^8.3",
        "laminas/laminas-http": "^2.21",
        "m1/env": "^2.2",
        "particle/validator": "^2.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Valerialevenets94\\ProxmoxLowBatteryShutdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Valerialevenets94\\ProxmoxLowBatteryShutdownTest\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Valerie",
            "email": "valeria.levenets94@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "phpspec/prophecy-phpunit": "^1.1"
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
